{
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "outputDirectory": "dist",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    }
  ],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://oaidalleapiprodscus.blob.core.windows.net/:path*"
    },
    {
      "source": "/__/auth/:path*",
      "destination": "https://ideal-type-8ba18.firebaseapp.com/__/auth/:path*"
    }
  ]
}
